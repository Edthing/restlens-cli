{
  "name": "restlens-cli",
  "version": "0.1.0",
  "description": "CLI for REST Lens API evaluation",
  "type": "module",
  "bin": {
    "restlens": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format esm --dts --clean",
    "dev": "tsup src/cli.ts --format esm --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "restlens",
    "api",
    "openapi",
    "swagger",
    "cli"
  ],
  "license": "GPL-3.0-only"
}
