{
  "name": "@restlens/cli",
  "version": "0.2.0",
  "description": "CLI for REST Lens API evaluation",
  "type": "module",
  "bin": {
    "restlens": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format esm --dts --clean",
    "dev": "tsup src/cli.ts --format esm --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@restlens/lib": "^0.1.0",
    "commander": "^12.1.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Edthing/restlens-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Edthing/restlens-cli/issues"
  },
  "homepage": "https://github.com/Edthing/restlens-cli#readme",
  "keywords": [
    "restlens",
    "api",
    "openapi",
    "swagger",
    "cli"
  ],
  "license": "GPL-3.0-only",
  "publishConfig": {
    "access": "public"
  }
}
